{
	"testRunner": "xunit",
	"userSecretsId": "ACs-Framework-Example",
	"dependencies": {
		"Microsoft.AspNetCore.Authentication.JwtBearer": "1.0.0",
		"Microsoft.AspNetCore.Authorization": "1.0.0",
		"Microsoft.AspNetCore.Diagnostics": "1.0.0",
		"Microsoft.AspNetCore.Mvc": "1.0.1",
		"Microsoft.AspNetCore.Razor.Tools": {
			"version": "1.0.0-preview2-final",
			"type": "build"
		},
		"Microsoft.AspNetCore.Routing": "1.0.1",
		"Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
		"Microsoft.AspNetCore.Server.Kestrel": "1.0.1",
		"Microsoft.AspNetCore.StaticFiles": "1.0.0",
		"Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0",
		"Microsoft.Extensions.Configuration.Json": "1.0.0",
		"Microsoft.Extensions.Logging": "1.0.0",
		"Microsoft.Extensions.Logging.Console": "1.0.0",
		"Microsoft.Extensions.Logging.Debug": "1.0.0",
		"Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
		"Microsoft.Extensions.Configuration.UserSecrets": "1.0.0",
		"Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0",
		"System.IdentityModel.Tokens.Jwt": "5.0.0",
		"xunit": "2.2.0-beta2-build3300",
		"dotnet-test-xunit": "2.2.0-preview2-build1029",
		"Moq": "4.5.23",
		"Microsoft.AspNetCore.SpaServices": "1.0.0-beta-000019"
	},

	"tools": {
		"BundlerMinifier.Core": "2.0.238",
		"Microsoft.AspNetCore.Razor.Tools": "1.0.0-preview2-final",
		"Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final",
		"Microsoft.Extensions.SecretManager.Tools": "1.0.0-preview2-final"
	},

	"frameworks": {
		".NETFramework4.5.1": {
			"dependencies": {
				"NHibernate": "4.0.4.4000",
				"FluentNHibernate": "2.0.3",
				"FluentMigrator": "1.6.2",
				"ACs.NHibernate": "1.0.0-*",
				"ACs.Net.Mail": "1.0.0-*",
				"ACs.Security": "1.0.0-*",
				"ACs.Security.Jwt": "1.0.0-*",
				"ACs.NHibernate.AspnetCore": "1.0.0-*",
				"ACs.Angular.AspnetCore": "1.0.0-*",
				"ACs.Misc": "1.0.0-*",
				"ACs.ErrorHandler.AspnetCore": "1.0.0-*"
			},
			"frameworkAssemblies": {
				"Microsoft.CSharp": "4.0.0.0",
				"System.Data": "4.0.0.0",
				"System.Linq.Expressions": "4.0.0.0",
				"System.Collections": "4.0.0.0",
				"System.Reflection": "4.0.0.0"
			}
		}
	},

	"commands": {
		"run": "run server.urls=http://localhost:5003",
		"web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5000",
		"weblistener": "Microsoft.AspNet.Hosting --server WebListener --server.urls http://localhost:5004"
	},
	"buildOptions": {
		"emitEntryPoint": true,
		"preserveCompilationContext": true
	},

	"runtimeOptions": {
		"configProperties": {
			"System.GC.Server": true
		}
	},

	"publishOptions": {
		"include": [
			"wwwroot",
			"**/*.cshtml",
			"appsettings.json",
			"web.config"
		]
	},

	"scripts": {
		"prepublish": [ "bower install", "dotnet bundle" ],
		"postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
	}
}
